@import "../../assets/fonts/iconfont.wxss";

.navbar{
  position: fixed;
  top: 0;
  width: 100%;
  height: 64px;
  line-height: 81px;
  background: white;
  text-align: center;
  font-weight: bold;
  font-size: 19px;
  z-index: 999;
}

.nav-back{
  position: fixed;
  top: 27px;
  left: 6px;
  height: 26px;
  width: 26px;
  z-index:9999;
  overflow: hidden;
}

.nav-back-mask{
  display: none;
  position: absolute;
  background: black;
  height: 26px;
  width: 26px;
  border-radius: 50%;
  opacity: 0.4;
}

.navbar-icon{
  position: fixed;
  top: 27px;
  left: 6px;
  color: black;
  height: 26px;
  line-height: 26px;
  width: 26px;
  text-align: center;
  font-size: 19px;
  font-weight: bold;
}
